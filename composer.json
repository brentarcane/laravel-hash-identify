{
    "name": "brentkozjak/laravel-hash-identify",
    "description": "Laravel package to easily identify possible algorithm(s) of a hash",
    "keywords": [
        "hashing",
        "hash",
        "algorithm"
    ],
    "homepage": "https://github.com/brentkozjak/laravel-hash-identify",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Brent Kozjak"
        }
    ],
    "require": {
        "php": "^7.0",
        "brentkozjak/hash-identify": "~1.0.0",
        "illuminate/support": "5.5.*|5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0.*|7.0.*",
        "orchestra/testbench": "3.5.*|3.6.*"
    },
    "autoload": {
       "psr-4": {
            "BrentKozjak\\LaravelHashIdentify\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BrentKozjak\\LaravelHashIdentify\\HashIdentifyServiceProvider"
            ],
            "aliases": {
                "Hash": "BrentKozjak\\LaravelHashIdentify\\HashIdentifyFacade"
            }
        }
    }
}
